shared:shacl {
    shared:shapes dcterms:hasVersion "0.1.0"^^xsd:string .

    #
    # Define the SHACL for OLAPDLIB's use of rdfs:label
    #
    rdfs:labelShape a sh:PropertyShape ;
        sh:path rdfs:label ;
        dcterms:hasVersion "1.0.0"^^xsd:string ;
        dcterms:creator <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:created "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        dcterms:contributor <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:modified "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        sh:datatype rdf:langString ;
        sh:languageIn ( "en" "de" "fr" "it" ) ;
        sh:uniqueLang true ;
        sh:name "Label"@en,  "Label"@de, "Label"@fr, "Label"@it ;
        sh:description "A human readable name for something."@en .

    #
    # Define SHACl for OLDAPLIB's use rdf:description
    #
    rdfs:commentShape a sh:PropertyShape ;
        sh:path rdfs:comment ;
        dcterms:hasVersion "1.0.0"^^xsd:string ;
        dcterms:creator <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:created "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        dcterms:contributor <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:modified "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        sh:datatype rdf:langString ;
        sh:languageIn ( "en" "de" "fr" "it" ) ;
        sh:uniqueLang true ;
        sh:name "Description"@en ;
        sh:description "A description for something."@en .

    #--------------------------------------------------------------------------
    # dcterms properties
    #

    dcterms:descriptionShape a sh:PropertyShape ;
        sh:path dcterms:description ;
        dcterms:hasVersion "1.0.0"^^xsd:string ;
        dcterms:creator <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:created "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        dcterms:contributor <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:modified "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        sh:name "Description"@en, "Beschreibung"@de, "Description"@fr, "Descrizione"@it ;
        sh:description "Description may include but is not limited to: an abstract, a table of contents, a graphical representation, or a free-text account of the resource."@en,
                           "Die Beschreibung kann eine Zusammenfassung, ein Inhaltsverzeichnis, eine grafische Darstellung oder eine Freitextbeschreibung der Ressource umfassen, ist aber nicht darauf beschränkt."@de,
                           "La description peut inclure, sans s'y limiter, un résumé, une table des matières, une représentation graphique ou un compte rendu en texte libre de la ressource."@fr,
                           "La descrizione può includere, a titolo esemplificativo ma non esaustivo: un abstract, un indice, una rappresentazione grafica o un resoconto a testo libero della risorsa."@it ;
        sh:datatype rdf:langString .

    dcterms:rightsShape a sh:PropertyShape ;
        sh:path dcterms:rights ;
        dcterms:hasVersion "1.0.0"^^xsd:string ;
        dcterms:creator <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:created "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        dcterms:contributor <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:modified "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        sh:name "Rights"@en, "Rechte"@de, "Droits"@fr, "Diritti"@it ;
        sh:description "Typically, rights information includes a statement about various property rights associated with the resource, including intellectual property rights. Recommended practice is to refer to a rights statement with a URI. If this is not possible or feasible, a literal value (name, label, or short text) may be provided."@en,
                       "In der Regel enthalten die Informationen zu den Rechten eine Erklärung zu den verschiedenen mit der Ressource verbundenen Eigentumsrechten, einschließlich der Rechte an geistigem Eigentum. Es wird empfohlen, mit einem URI auf eine Rechteerklärung zu verweisen. Ist dies nicht möglich oder praktikabel, kann ein literarischer Wert (Name, Bezeichnung oder Kurztext) angegeben werden."@de,
                       "En général, les informations sur les droits comprennent une déclaration sur les divers droits de propriété associés à la ressource, y compris les droits de propriété intellectuelle. La pratique recommandée est de faire référence à une déclaration de droits avec un URI. Si cela n'est pas possible ou réalisable, une valeur littérale (nom, étiquette ou texte court) peut être fournie."@fr,
                       "In genere, le informazioni sui diritti includono una dichiarazione sui vari diritti di proprietà associati alla risorsa, compresi i diritti di proprietà intellettuale. La prassi consigliata è quella di fare riferimento a una dichiarazione sui diritti con un URI. Se ciò non è possibile o fattibile, si può fornire un valore letterale (nome, etichetta o testo breve)."@it ;
        sh:datatype xsd:string .

    dcterms:abstractShape a sh:PropertyShape ;
        sh:path dcterms:abstract ;
        dcterms:hasVersion "1.0.0"^^xsd:string ;
        dcterms:creator <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:created "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        dcterms:contributor <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:modified "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        sh:name "Abstract"@en, "Abstrakt"@de, "Résumé"@fr, "Astratto"@it ;
        sh:description
            "A summary of the resource."@en,
            "Eine Zusammenfassung der Ressource."@de,
            "Un résumé de la ressource."@fr,
            "Una sintesi della risorsa."@it ;
        rdfs:subPropertyOf dcterms:description ;
        sh:datatype rdf:langString .

    dcterms:accessRightsShape a sh:PropertyShape ;
        sh:path dcterms:accessRights ;
        dcterms:hasVersion "1.0.0"^^xsd:string ;
        dcterms:creator <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:created "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        dcterms:contributor <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:modified "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        sh:name "Access Rights"@en, "Zugangsrechte"@de, "Droits d'accès"@fr, "Diritti di accesso"@it ;
        sh:description
                "Information about who access the resource or an indication of its security status."@en,
                "Informationen darüber, wer auf die Ressource zugreift, oder ein Hinweis auf ihren Sicherheitsstatus."@de,
                "Informations sur les personnes qui accèdent à la ressource ou indication de son état de sécurité."@fr,
                "Informazioni su chi accede alla risorsa o un'indicazione del suo stato di sicurezza."@it ;
        rdfs:subPropertyOf dcterms:rights ;
        sh:datatype xsd:string .


    #
    # Definition of SHACL for OLDAP's use of dcterms:creator. Points an oldap:User .
    #
    dcterms:creatorShape a sh:PropertyShape ;
        sh:path dcterms:creator ;
        dcterms:hasVersion "1.0.0"^^xsd:string ;
        dcterms:creator <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:created "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        dcterms:contributor <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:modified "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        sh:class oldap:User .

    #
    # Definitionm of SHACL for OLDAPLIB's use of dcterms:created
    #
    dcterms:createdShape a sh:PropertyShape ;
        sh:path dcterms:created ;
        dcterms:hasVersion "1.0.0"^^xsd:string ;
        dcterms:creator <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:created "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        dcterms:contributor <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:modified "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        sh:datatype xsd:dateTime .

    #
    # Define SHACL for OLDAPLIB's use of dcterms:contributer. Points an oldap:User .
    #
    dcterms:contributorShape a sh:PropertyShape ;
        sh:path dcterms:contributor ;
        dcterms:hasVersion "1.0.0"^^xsd:string ;
        dcterms:creator <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:created "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        dcterms:contributor <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:modified "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        sh:class oldap:User .

    #
    # Define SHACL of OLDAPLIB's use of dcterms:modified
    #
    dcterms:modifiedShape a sh:PropertyShape ;
        sh:path dcterms:modified ;
        dcterms:creator <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:created "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        dcterms:contributor <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:modified "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        sh:datatype xsd:dateTime .

    dcterms:PhysicalResourceShape a sh:NodeShape, dcterms:PhysicalResource ;
        sh:targetClass dcterms:PhysicalResource ;
        dcterms:creator <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:created "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        dcterms:contributor <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:modified "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        rdfs:label "Physical Resource"@en, "Physische Ressource"@de, "Ressources physiques"@fr, "Risorse fisiche"@it ;
        rdfs:comment "A material thing."@en, "Eine materielle Sache."@de, "Une chose matérielle."@fr, "Una cosa materiale."@it ;
        sh:property
            [
                sh:path rdf:type ;
            ] .


    schema:familyNameShape a sh:PropertyShape ;
        sh:path schema:familyName ;
        dcterms:creator <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:created "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        dcterms:contributor <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:modified "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        sh:datatype xsd:string ;
        sh:name "Family name"@en, "Familiennamen"@de, "Nom de famillie"@fr, "Nome della famiglia"@it ;
        sh:description "The family name of some person."@en .

    schema:givenNameShape a sh:PropertyShape ;
        sh:path schema:givenName ;
        dcterms:creator <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:created "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        dcterms:contributor <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:modified "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        sh:datatype xsd:string ;
        sh:name "Given name"@en, "Vornamen"@de, "Prénom"@fr, "Nome"@it ;
        sh:description "The given name of some person"@en .

    schema:emailShape a sh:PropertyShape ;
        sh:path schema:email ;
        dcterms:creator <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:created "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        dcterms:contributor <https://orcid.org/0000-0003-1681-4036> ;
        dcterms:modified "2023-11-04T12:00:00+00:00"^^xsd:dateTime ;
        sh:datatype xsd:string ;
        sh:name "Email"@en, "Email"@de, "Courriel"@fr, "Email"@it ;
        sh:description "The email adress of some person"@en .

}

