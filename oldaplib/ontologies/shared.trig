shared:shacl {

    #
    # Define the SHACL for OLAPDLIB's use of rdfs:label
    #
    rdfs:labelShape a sh:PropertyShape ;
        sh:path rdfs:label ;
        dcterms:hasVersion "1.0.0"^^xsd:string ;
        dcterms:creator <https://orcid.org/0000-0003-1681-4036> ;
